object Person {
    str name,
    num age = 18,

    fun growUp() > num {
        this.age += 1;

        return this.age;
    }
}

fun main() {
    Person me = Person{
        name = "Giann",
        age = 35,
    };

    print("Hello I'm {me.name} and I'm {me.age}.");
}