import "lib/std";

fun jitSayHello(str who, int age) > int {
    if (age > 12) {
        print("yeah if!");
    } else {
        print("yeah else!");
    }
    print("Hello {who} you're {age}!");

    [int] list = [1, 2, 3];

    print("list {list}");

    | print("list has {list.len()} elements, second element is {list[1]}");

    | list[1] = 4;

    | print("now it's {list[1]}");

    return 12;
}

fun main([str] args) > void {
    print("Got: {jitSayHello("Joe", age: 36)}");
}