import "lib/std";

test "list.forEach" {
    [int] data = [1, 2, 3, 4];

    int sum = 0;
    data.forEach(fun (int element) > void {
        sum = sum + element;
    });

    assert(sum == 10, message: "Could use forEach");
}

test "list.map" {
    [int] data = [1, 2, 3, 4];

    [str] mapped = data.map(<str>, fun (int element) -> "{element}");

    print(mapped.join(", "));
}